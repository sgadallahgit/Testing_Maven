pipeline {
    agent any

    tools { 
      maven 'MAVEN_HOME' 
      jdk 'JAVA_HOME' 
    }
    stages {
        stage('Build') {
            steps {
                echo 'This is a Build Stage'
                sh 'mvn clean install'
            }
        }
        stage('Test') {
            steps {
                echo 'This is a Test Stage'
            }
        }
        stage('Deploy') {
            steps {
                echo 'This is a Deploy Stage'
                sh 'mvn clean install'
            }
        }
    }
}
