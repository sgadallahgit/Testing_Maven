pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'This is a Build Stage'
                
                mavenJob('Maven_DSL_Job_2') {
                    description('Maven Job')
                    label('New Node')
                    scm {
                        git("https://github.com/sgadallahgit/Testing_Maven.git", '*/master')
                    }
                    goals('clean install')
                }
            }
        }
        stage('Test') {
            steps {
                echo 'This is a Test Stage'
            }
        }
        stage('Deploy') {
            steps {
                echo 'This is a Deploy Stage'
            }
        }
    }
}
